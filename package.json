{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w dist dist/server.js",
    "compile-server": "babel server --presets es2015-node4 --out-dir dist --source-maps",
    "watch-server": "babel server --presets es2015-node4 --out-dir dist --source-maps --watch",
    "start-hook": "webpack & nodemon -w server server/start_hook.js",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot",
    "dev-all": "npm run watch & npm run watch-server & npm start",
    "dev-all-hook": "npm run watch & npm run start-hook"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "express": "^4.17.2",
    "mongo": "^0.1.0",
    "mongo-client": "^0.2.1",
    "mongodb": "^4.3.0",
    "mongoose": "^6.1.6",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.2.1",
    "source-map-support": "^0.5.21",
    "with-router": "^1.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "whatwg-fetch": "^3.6.2"
  }
